}

.flickity-button:hover{
  cursor:pointer;
  opacity:1;
}

.flickity-button:disabled{
  cursor:auto;
  display:none;
  pointer-events:none;
}

.flickity-prev-next-button{
  height:40px;
  top:50%;
  transform:translateY(-50%);
  width:40px;
}

